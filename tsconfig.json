{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "WebWorker"],
    "module": "ES2022",
    "moduleResolution": "node",
    "outDir": "dist",
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022"
  }
}
